{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "inboundplaytime",
						"type": "DatasetReference"
					},
					"name": "sourceplaytime"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					},
					"name": "sinkplaytime",
					"rejectedDataLinkedService": {
						"referenceName": "AzureDataLakeStorage",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          appid as integer,",
				"          playtime_forever as integer,",
				"          playtime_windows_forever as integer,",
				"          playtime_mac_forever as integer,",
				"          playtime_linux_forever as integer,",
				"          playtime_deck_forever as integer,",
				"          rtime_last_played as integer,",
				"          playtime_disconnected as integer,",
				"          playtime_2weeks as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'arrayOfDocuments',",
				"     mode: 'read') ~> sourceplaytime",
				"sourceplaytime sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkplaytime"
			]
		}
	}
}